# Test environment variables
# Copy this file to .env.test for running tests

# Database configuration for testing
TEST_DATABASE_URL=mysql://user:password@ip:port/db

# Lark API credentials for testing (use dummy values for most tests)
TEST_LARK_APP_ID=test_app_id
TEST_LARK_APP_SECRET=test_app_secret

# JWT secret for testing
TEST_JWT_SECRET=test_jwt_secret_for_testing_only

# Log level for tests
TEST_LOG_LEVEL=info
# Timezone configuration
TIMEZONE_OFFSET_SECS=0
